{
    "name": "__MSG_extName__",
    "description": "__MSG_extDescription__",
    "version": "0.1",
    "default_locale": "zh_TW",
    "offline_enabled": true,
    "permissions": [
        "cookies",
        "tabs",
        "<all_urls>",
        "http://www.google.com.tw/"
    ],
    "icons": {
        "16": "icon16.png",
        "128": "icon128.png"
    },
    "background": {
        "scripts": [ "background.js" ],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Txt Formatter"
    },
    "manifest_version": 2
}
